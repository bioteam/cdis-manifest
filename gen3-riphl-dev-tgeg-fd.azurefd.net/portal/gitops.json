  "explorerConfig":[
    {
      "tabTitle":"Cases",
      "charts":{
        "projects_code":{
          "chartType":"count",
          "title":"Projects"
        },
        "_case_id":{
          "chartType":"count",
          "title":"Cases"
        },
        "Submission_Type": {
          "chartType": "bar",
          "title": "Submission Type"
        }
      },
      "filters":{
        "tabs":[
          {
            "title":"Cases",
            "fields":[
              "projects_code",
              "submitter_id",
              "Submission_Type",
              "Collection_Date"
            ]
          }
        ]
      },
      "table":{
        "enabled": true,
        "fields":[
          "project_id",
          "submitter_id",
          "projects_code",
          "Collection_Date",
          "DOB",
          "First_Name",
          "Last_Name",
          "Original_Specimen_Type",
          "Submission_Type",
          "Transport_Media",
          "Transport_Other"
        ]
      },
      "buttons":[ 
        {
          "enabled": true,
          "type": "data-tsv",
          "title": "Download Selected Cases as case.tsv",
          "leftIcon": "user",
          "rightIcon": "download",
          "fileName": "case.tsv"
        }
      ],
      "guppyConfig":{
        "dataType":"case",
        "nodeCountTitle":"Cases",
        "fieldMapping": [
          { "field": "project_id", "name": "Project" },
          { "field": "submitter_id", "name": "RIPHL ID" },
          { "field": "projects_code", "name": "Partner" },

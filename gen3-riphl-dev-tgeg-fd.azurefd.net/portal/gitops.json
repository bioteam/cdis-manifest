          { "field": "Last_Name", "name": "Last Name" },
          { "field": "DOB", "name": "DOB" },
          { "field": "Original_Specimen_Type", "name": "Original Specimen Type" },
          { "field": "Submission_Type", "name": "Submission Type" },
          { "field": "Transport_Media", "name": "Transport Media" },
          { "field": "Transport_Other", "name": "Transport Other" }
        ],
        "manifestMapping": {
          "referenceIdFieldInResourceIndex": "_case_id"
        }
      }
    },
